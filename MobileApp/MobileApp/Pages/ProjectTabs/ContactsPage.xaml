<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApp.ContactsPage"
             Visual="Material">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <Entry x:Name="phoneEntry" Placeholder="Телефон №1" Keyboard="Telephone" IsReadOnly="True"/>
            </StackLayout>
            <StackLayout>
                <Entry x:Name="emailntry" Placeholder="E-mail" Keyboard="Email" IsReadOnly="True"/>
            </StackLayout>
            <StackLayout>
                <Entry x:Name="addressEntry" Placeholder="Адрес" IsReadOnly="True"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>